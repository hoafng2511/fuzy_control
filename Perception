      "source": [
        "import numpy as np\n",
        "!pip install ipywidgets\n",
        "!pip install gradio\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.linear_model import Perceptron\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.metrics import accuracy_score\n",
        "import ipywidgets as widgets\n",
        "from IPython.display import display, clear_output\n",
        "import pandas as pd\n",
        "data = pd.read_csv('/content/diabetes.csv')\n",
        "X = data.drop(\"Outcome\", axis=1)\n",
        "y = data[\"Outcome\"]\n",
        "\n",
        "# Chuẩn hóa dữ liệu\n",
        "scaler = StandardScaler()\n",
        "X_scaled = scaler.fit_transform(X)\n",
        "\n",
        "# Chia dữ liệu\n",
        "X_train, X_test, y_train, y_test = train_test_split(X_scaled, y, test_size=0.2, random_state=42)\n",
        "\n",
        "# Huấn luyện mô hình Logistic Regression\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "import gradio as gr\n",
        "\n",
        "model = LogisticRegression(max_iter=200)\n",
        "model.fit(X_train, y_train)\n",
        "\n",
        "# Lấy thông tin min/max để đặt range thực tế\n",
        "col_min = data.min()\n",
        "col_max = data.max()\n",
        "\n",
        "def du_doan_tieu_duong(preg, glucose, blood, skin, insulin, bmi, dpf, age):\n",
        "    input_data = np.array([[preg, glucose, blood, skin, insulin, bmi, dpf, age]])\n",
        "    input_scaled = scaler.transform(input_data)\n",
        "    prediction = model.predict(input_scaled)[0]\n",
        "    probability = model.predict_proba(input_scaled)[0][1]\n",
        "    if prediction == 1:\n",
        "        return f\"⚠️ Nguy cơ MẮC bệnh tiểu đường.\\nXác suất: {probability*100:.2f}%\"\n",
        "    else:\n",
        "        return f\"✅ KHÔNG có nguy cơ mắc bệnh tiểu đường.\\nXác suất: {probability*100:.2f}%\"\n",
        "\n",
        "# Giao diện với range thực tế từ dữ liệu\n",
        "interface = gr.Interface(\n",
        "    fn=du_doan_tieu_duong,\n",
        "    inputs=[\n",
        "        gr.Slider(minimum=int(col_min['Pregnancies']), maximum=int(col_max['Pregnancies']), step=1, label=\"Số lần mang thai\"),\n",
        "        gr.Slider(minimum=int(col_min['Glucose']), maximum=int(col_max['Glucose']), step=1, label=\"Đường huyết (Glucose)\"),\n",
        "        gr.Slider(minimum=int(col_min['BloodPressure']), maximum=int(col_max['BloodPressure']), step=1, label=\"Huyết áp (mmHg)\"),\n",
        "        gr.Slider(minimum=int(col_min['SkinThickness']), maximum=int(col_max['SkinThickness']), step=1, label=\"Độ dày da (mm)\"),\n",
        "        gr.Slider(minimum=int(col_min['Insulin']), maximum=int(col_max['Insulin']), step=1, label=\"Insulin (mu U/ml)\"),\n",
        "        gr.Slider(minimum=float(col_min['BMI']), maximum=float(col_max['BMI']), step=0.1, label=\"BMI\"),\n",
        "        gr.Slider(minimum=float(col_min['DiabetesPedigreeFunction']), maximum=float(col_max['DiabetesPedigreeFunction']), step=0.01, label=\"Chỉ số di truyền (DPF)\"),\n",
        "        gr.Slider(minimum=int(col_min['Age']), maximum=int(col_max['Age']), step=1, label=\"Tuổi\")\n",
        "    ],\n",
        "    outputs=\"text\",\n",
        "    title=\"🩺 Dự đoán bệnh tiểu đường\",\n",
        "    description=\"Nhập các thông số sức khỏe để dự đoán nguy cơ tiểu đường dựa trên mô hình Logistic Regression.\"\n",
        ")\n",
        "\n",
        "interface.launch()\n"
      ]
