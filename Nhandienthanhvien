    {
      "cell_type": "code",
      "source": [
        "!pip install gradio\n",
        "\n",
        "import gradio as gr\n",
        "import cv2\n",
        "import numpy as np\n",
        "from keras.models import load_model\n",
        "from PIL import Image\n",
        "\n",
        "# 1. Load model ANN đã train\n",
        "model = load_model(\"face_ann.h5\")\n",
        "\n",
        "# 2. Mapping class (theo class_indices khi train)\n",
        "classes = {'NguyenVanHoa': 0, 'NguyenDuongNhatHuy': 1, 'VoHoangKhang': 2}\n",
        "\n",
        "# 3. Thông tin sinh viên\n",
        "info_list = [\n",
        "    \"Nguyễn Văn Hòa - MSSV 31241021473 - Sinh ngày 25/11/2006 - Lớp ICA\",\n",
        "    \"Lê Nhật Huy - MSSV 31231026121 - Sinh ngày 5/11/2005 - Lớp Robot\",\n",
        "    \"Võ Hoàng Khang - MSSV 31241021883 - Sinh ngày 9/11/2006 - Lớp ICA\"\n",
        "]\n",
        "\n",
        "# 4. Hàm dự đoán\n",
        "def predict(img):\n",
        "    img = np.array(img.convert(\"RGB\"))  # PIL -> numpy\n",
        "    img_resized = cv2.resize(img, (60, 60)) / 255.0\n",
        "    img_ready = np.expand_dims(img_resized, axis=0)\n",
        "\n",
        "    pred = model.predict(img_ready)\n",
        "    class_id = np.argmax(pred)\n",
        "    class_name = list(classes.keys())[class_id]\n",
        "    class_info = info_list[class_id]\n",
        "\n",
        "    return class_info\n",
        "\n",
        "# 5. Tạo app Gradio\n",
        "demo = gr.Interface(\n",
        "    fn=predict,\n",
        "    inputs=gr.Image(type=\"pil\", label=\"Upload ảnh khuôn mặt\"),\n",
        "    outputs=\"text\",\n",
        "    title=\"Nhận diện thành viên nhóm 7\",\n",
        "    description=\"Upload ảnh để nhận diện sinh viên\"\n",
        ")\n",
        "\n",
        "demo.launch(share=True)\n"
      ],
